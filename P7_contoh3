#include <iostream>

using namespace std;

int main(){
    char c;
    char i;

    cout<<"Naik broo....."<<endl<<endl;
    for(i='A';i<='J';i++){
        if(i=='J'){
            cout<<i;
        }else{
        cout<<i<<",";}
    }
    cout<<endl<<endl<<"Menurunnya nih .... "<<endl<<endl;

    for(c='I';c>='A';c--){
            if(c=='A'){
            cout<<c;
        }else{
        cout<<c<<",";}
    }
    cout<<endl;


return 0;
}
